
@{
    ViewData["Title"] = "Menu";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header card-header-danger card-header-icon">
                        <div class="card-icon">
                            <i class="material-icons">menu</i>
                        </div>
                        <h4 class="card-title">Menü Yönetimi</h4>
                    </div>
                    <div class="card-body">
                        <div class="toolbar">
                            <a class="btn btn-link" data-mode="GetHaberFormModal" data-data1="0"><i class="material-icons">save</i> Ekle</a>
                            <a href="#" class="btn btn-link" data-data1=""><i class="material-icons">edit</i> İşlem</a>
                        </div>


 
                    </div>
                    <!-- end content-->
                </div>
                <!--  end card  -->
            </div>
            <!-- end col-md-12 -->
        </div>
        <!-- end row -->
    </div>
</div>

@section AdminScripts{

    <script>
        $(document).ready(function() {
              $.get("/Home/_Index", function(data, status){
                  if(data.success)
                  {
                        alert(data.message);
                  }
                  else
                  {
                      alert(data.message);
                      //Hatalı
                  }
        });
    </script>
}

