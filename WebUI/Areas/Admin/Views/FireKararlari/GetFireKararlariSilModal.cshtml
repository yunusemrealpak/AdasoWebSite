@using WebApplication1.Models
@using WebUI.Models
@using WebUI.Models.Api
 

<div class="modal-content">
    <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Silme işlemi</h5>
        <button class="close" type="button" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
    </div>
    <div class="modal-body">@TempData["id"]  numaralı kaydı silmek istediğinize emin misiniz?</div>
    <div class="modal-footer">
        <button class="btn btn-outline-secondary" type="button" data-dismiss="modal">İptal</button>
        <a class="btn btn-danger" data-mode="GetFireKararlariIslemler" data-type="Sil" data-id="@TempData["id"]">SİL</a>
    </div>
</div>